@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
        <h2>Register !</h2>
        @Html.Partial("./SignUpFormular.cshtml")

    </div>

    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
        <h2>Already Registered ?</h2>
        <a asp-controller="Authentification" asp-action="SignIn"><button class="btn btn-primary">SignIn</button></a>

    </div>

</div>
